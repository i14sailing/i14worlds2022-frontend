{
  "name": "tack-and-gybe-frontend",
  "description": "Gatsby/TailwindCSS frontend for the I14 Tack and Gybe Challenge",
  "scripts": {
    "build": "tailwind build src/styles/tailwind.css -o src/styles/tailwind.out.css && gatsby build",
    "build:tailwind": "tailwind build src/styles/tailwind.css -o src/styles/tailwind.out.css",
    "develop": "gatsby develop -H 0.0.0.0 -p 8000",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^2.26.1",
    "gatsby-plugin-layout": "^1.8.0",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-sharp": "^2.13.2",
    "gatsby-source-strapi": "^0.0.12",
    "gatsby-transformer-sharp": "^2.11.0",
    "lodash": "^4.17.20",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "typeface-libre-franklin": "^1.1.13",
    "typeface-quicksand": "^1.1.13"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "autoprefixer": "^10.2.1",
    "gatsby-plugin-root-import": "^2.0.5",
    "postcss": "^8.2.4",
    "prettier": "2.2.1",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3"
  }
}
